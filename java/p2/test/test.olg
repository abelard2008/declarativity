import java.lang.Integer;
import java.lang.Float;
import p2.types.table.Index;

define(t1, infinity, infinity, keys(1), 
 {int, string, int, int[5][6]});

define(t2, infinity, infinity, keys(), 
 {Integer, Float, Float, Float });

define(event, {Integer, Float, Float});

watchmod(t1, a);
watchmod(t2, s);

rule1 event(A, B, C) :- t1(@A, F:=$2), 
			C > 0,
			t2(@A, B, C, D), 
                        X:= {{1,2},{3,4}}, 
			F.toString() == "foobar";

rule2 delete t2(@A, B, C, D) :- event(@A, B, C), 
                                notin t2(@A, B, C, 2.0), 
				F := Index.Type.SECONDARY.toString(),
                                C in [1, 5],
                                D := C < 2 ? (C < 1 ? 0f : 2.0) : 1.0;
