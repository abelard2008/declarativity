<?xml version="1.0" encoding="ISO-8859-1"?> 

<!DOCTYPE web-app
   PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
   "http://java.sun.com/dtd/web-app_2_3.dtd">


<web-app>

    <servlet>
        <servlet-name>Civil</servlet-name><!-- Common gateway Interface for VIewing Lincoln-->
        <servlet-class>jol.net.servlet.Civil</servlet-class>
	<init-param>
	  <param-name>port</param-name>
	  <param-value>8747</param-value> <!-- 87 = 4 score + 7 -->
	</init-param> 
	<init-param>
	  <param-name>olg</param-name>
	  <param-value>path.olg</param-value>
	</init-param>
    </servlet>
    <servlet-mapping>
      <servlet-name>Civil</servlet-name>
      <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
